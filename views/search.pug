html
    body
        table
            tr
                th First Name
                th Last Name
                th Email
                th Address
                th Age
            each user, index in monUser
                tr
                    td #{user.firstName}
                    td #{user.lastName}
                    td #{user.email}
                    td #{user.address}
                    td #{user.age}
        div
            a(href="/") Return